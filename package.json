{
  "name": "@trademe/ng-add-to-calendar",
  "version": "14.0.0",
  "description": "Angular library to add custom events to common calendar applications",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Mike Francis",
  "license": "MIT",
  "keywords": [
    "angular",
    "javascript",
    "typescript"
  ],
  "scripts": {
    "clean": "rimraf ./dist ./compiled && mkdir dist",
