  },
  "devDependencies": {
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/platform-browser": "^14.2.12",
    "core-js": "^3.6.5",
    "rxjs": "~7.5.7",
    "zone.js": "~0.11.8",
    "@angular/cli": "14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@angular/language-service": "^14.2.12",
    "@types/node": "10.17.45",
    "codelyzer": "^6.0.2",
    "rimraf": "^3.0.0",
    "tslint": "^6.1.3",
    "typescript": "~4.8.4",
    "ng-packagr": "^14.2.2",    
    "tslib": "^2.3.0"
  }
